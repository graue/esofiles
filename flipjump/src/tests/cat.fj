    startup
start:
    bit.input ascii
    bit.if0 8, ascii, end
    bit.cmp 8, ascii, nl, final_check, end, final_check
  final_check:
    bit.cmp 8, ascii, cr, print, end, print
print:
    bit.print ascii
    ;start
end:
    loop


ascii:
    bit.vec 8, 0
nl:
    bit.vec 8, '\n'
cr:
    bit.vec 8, '\r'
